<?xml version="1.0" encoding="UTF-8"?>
<refinfo>
   <!-- multiple versions permitted -->
   <!-- Other toolchain supported in the future
    <toolchain>
        <brand>IAR</brand>
        <product>ICCRX</product>
        <version>3.10</version>
    </toolchain>
    <toolchain>
      <brand>Renesas</brand>
      <product>RXC</product>
      <version>3.01.00</version>
   </toolchain>
    -->
   <toolchain>
      <brand>GNU</brand>
      <product>GNURX</product>
      <version>4.8.4</version>
   </toolchain>
   <!-- only one target entry allowed; multiple sub tags permitted -->
   <target>
      <toolchain>GNURX</toolchain>
      <family>RX</family>
      <series>RX600</series>
      <group>RX65N</group>
   </target>
   <!-- only one package entry allowed -->
   <!-- multiple dependencies, applications, functions, and incdirs permitted -->
   <package>
      <type>rtosmodule</type>
      <name>threadx</name>
      <version>6.1.3_rel-rx-1.0.0</version>
      <application>AzureRTOSKernel</application>
      <category>rtos.kernel</category>
      <desc>text</desc>
      <details>text</details>
      <function>RTOS</function>
      <configgenpath>
         <path>libs/threadx/tx_user.h</path>
      </configgenpath>
      <!-- Copy resources to project -->
      <impdir>
         <toolchain>GNURX</toolchain>
         <group>RX65N</group>
         <folder>threadx/ports/rxv2/gnu</folder>
         <path>libs/threadx/ports/rxv2/gnu</path>
      </impdir>
      <impdir>
         <folder>threadx/common</folder>
         <path>libs/threadx/common</path>
      </impdir>
      <impdir>
         <file>threadx/samples/demo_threadx.c</file>
         <path>src</path>
      </impdir>
      <impdir>
         <file>configuration/config_files/tx_user.h</file>
         <path>libs/threadx</path>
      </impdir>
      <!-- include header-->
      <incdir>
         <toolchain>GNURX</toolchain>
         <folder>libs/threadx/ports/rxv2/gnu/inc</folder>
      </incdir>
      <incdir>
         <folder>libs/threadx/common/inc</folder>
      </incdir>
   </package>
</refinfo>